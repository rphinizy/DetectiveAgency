@{
    ViewData["Title"] = "ABC Corporation Staff";
    string[] staff = { "Freddie Flintstone", "Barney Rubble", "Betty Rubble", "Wilma Flintstone" };
    int staffLen = staff.Length;
    string[] managers = { "Pebbles Flintstone", "Johnny Quest", "Shaggy Rogers" };
    int managerLen = managers.Length;
    string[] officers = { "Scooby Doo", "Astro Jetson", "Scrappy Doo" };
    int officerLen = officers.Length;
    var x = 0;
}
<hr/>
<div class="jumbotron bg-warning text-white row" style="max-width:70%; border-radius: 30px; margin:auto">
    <h1 class="col-sm-8 text-white">Meet the Team!</h1>
    <img src="~/Images/detectives.jpg" alt="xyz detectives" class="float-right rounded-circle mt-0 img-fluid col-sm-4" style="box-shadow:1px 1px black; margin-left 20%;" />
</div>
<br />
<div class="row" style="margin:auto; max-width:70%;">
    <i class="material-icons col-sm-1" style="font-size:38px;color:darkblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:lightblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:rosybrown">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:mediumpurple">group</i>
</div>
<table class="table table-hover table-striped bg-light">
    <tr class="table-warning"><th><h2>XYZ Detective Agency Staff</h2></th></tr>
    @for (x = 0; x < staffLen; x++)
    {
        <tr><td>@staff[x]</td></tr>
    }
</table>
<hr />
<div class="row" style="margin:auto; max-width:70%;">
    <i class="material-icons col-sm-1" style="font-size:38px;color:darkblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:lightblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:rosybrown">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:mediumpurple">group</i>
</div>
<table class="table table-hover table-striped bg-light">
    <tr class="table-warning"><th><h2>XYZ Detective Agency Managers</h2></th></tr>
    @foreach (var person in managers)
    {
        <tr><td>@person</td></tr>
    }
</table>
<hr />
<div class="row" style="margin:auto; max-width:70%;">
    <i class="material-icons col-sm-1" style="font-size:38px;color:darkblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:lightblue">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:rosybrown">group</i>
    <i class="material-icons col-sm-1" style="font-size:38px;color:mediumpurple">group</i>
</div>
<table class="table table-hover table-striped bg-light">
    <tr class="table-warning"><td><h2>XYZ Detective Agency Corporate Office</h2></td></tr>
    @{
        x = 0;
        while (x < officerLen)
        {
                                        <tr><td>@officers[x]</td></tr>
            x += 1;
        }
    }
</table>
<hr />
